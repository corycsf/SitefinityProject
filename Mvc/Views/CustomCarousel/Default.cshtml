@using Telerik.Sitefinity.DynamicModules.Model
@using Telerik.Sitefinity.Model

@model SitefinityWebApp.Mvc.Models.CustomCarouselModel
@{ 
    var images = Model.Images;
    int index = 0;
    int divIndex = 0;
    var imageCount = images.Count;
}
@Html.Hidden("imageCount", imageCount);
@foreach (var item in images)
{
    @Html.Hidden("image-url-" + index, item.Url)
    index++;
}
<div class="content-wrapper">
    <div class="content-panel main-content">
        <div class="content-head">
            <div class="car-wrapper">

                @foreach (var image in images)
                {
                    <div class="car-row">
                        <div id="car-image-@divIndex">
                            <span>@image.Title</span>
                        </div>
                    </div>
                    divIndex++;
                }
            </div>

        </div>
    </div>
</div>
<script type="text/javascript" src="//cdn.jsdelivr.net/jquery.slick/1.6.0/slick.min.js"></script>
<script type="text/javascript" src="~/Custom/Content/Scripts/carousel.js"></script>